 def dimage
pipeline {
  agent any
  stages{
    stage('package'){
       steps{
         echo "package- code"
         sh 'mvn clean package -DskipTests'
        }
      }
      stage('build'){
       steps{
       script{
        dimage = docker.build("ci-cd-demo")
       }
         echo "image build"
       }
      }
  }

 }